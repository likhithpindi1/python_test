class employee:
  def __init__(self, name, weeklyhours, rate, overtimeRate, weeklytaxcredit):
    self.name = name
    self.weeklyhours = weeklyhours
    self.rate = rate
    self.overtimeRate = overtimeRate
    self.weeklytaxcredit = weeklytaxcredit

  def computeWeeklyPay(self,hours):
    if (hours < self.weeklyhours):
      weekpay = hours * self.rate
    else:
    weekpay = (self.weeklyhours * self.rate ) + ((hours - self.weeklyhours) * self.overtimeRate )
    return weeklypay
    
      
  def computerTax(self,grossPay):
    grspay = (grosspay * (40.0 / 100))
    if (grspay > self.weeklytaxcredit ):
      tax_d = grspay-self.weeklytaxcredit
    else:
      tax-d = 0
       return tax_due


employee = Employee('Likith',20,11,10,5)
print(employee.computeweeklypay(10))
print(employee.computeTax(20))
     
    
    
  
  
  
